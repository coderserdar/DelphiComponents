package MagentaXfer7;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'Magenta Systems File Transfer Components Wide for Delphi 7 and ICS V7'}
{$IMPLICITBUILD OFF}
{$DEFINE USE_SSL}

requires
  rtl,
  vcl,
  inet,
  vclx,
  designide,
  vclactnband,
  magentasubs7,
  OverbyteIcsD7Design;

contains
  MagentaCopy in 'MagentaCopy.pas',
  MagentaFtp3 in 'MagentaFtp3.pas',
  MagentaHttp3 in 'MagentaHttp3.pas';

end.

