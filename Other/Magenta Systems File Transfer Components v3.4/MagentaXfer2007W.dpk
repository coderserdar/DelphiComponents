package MagentaXfer2007W;

{$R *.res}
{$R 'OverbyteIcsFtpCliW.dcr'}
{$R 'OverbyteIcsFtpSrvW.dcr'}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'Magenta Systems File Transfer Components Wide for Delphi 2007 and ICS V7'}
{$IMPLICITBUILD ON}
{$DEFINE USE_SSL}

requires
  rtl,
  vcl,
  inet,
  vclx,
  designide,
  vclactnband,
  xmlrtl,
  VCLZipPack,
  MagentaSubs11,
  OverbyteIcsD2007Design;

contains
  MagentaCopy3W in 'MagentaCopy3W.pas',
  MagentaFtp3W in 'MagentaFtp3W.pas',
  MagentaHttp3W in 'MagentaHttp3W.pas',
  OverByteIcsFtpSrvWT in 'OverByteIcsFtpSrvWT.pas',
  OverbyteIcsFtpCliW in 'OverbyteIcsFtpCliW.pas',
  OverbyteIcsFtpSrvW in 'OverbyteIcsFtpSrvW.pas';

end.

