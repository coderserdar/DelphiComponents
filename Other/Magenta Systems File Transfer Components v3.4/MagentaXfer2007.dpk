package MagentaXfer2007;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'Magenta Systems File Transfer Components for Delphi 2007 and ICS V7'}
{$IMPLICITBUILD ON}
{$DEFINE USE_SSL}

requires
  rtl,
  vcl,
  inet,
  vclx,
  designide,
  vclactnband,
  VCLZipPack,
  MagentaSubs11,
  OverbyteIcsD2007Design;

contains
  MagentaCopy in 'MagentaCopy.pas',
  MagentaFtp in 'MagentaFtp.pas',
  MagentaHttp in 'MagentaHttp.pas';

end.

